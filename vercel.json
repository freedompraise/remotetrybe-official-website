{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "redirects" : [
        {
          "source": "/",
          "destination": "https://www.remotetrybe.coom",
          "permanent": true
        }
      ]
    }
  ]
}